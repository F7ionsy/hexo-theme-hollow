<section class="jumbotron">
  <div class="video">
    <% if (theme.ocean.overlay) { %>
      <div class="video-frame">
        <img src="https://f7ionsy-1251389397.cos.ap-shanghai.myqcloud.com/background/overlay-hero.png" alt="Decorative image frame">
      </div>
    <% } %>
    <div class="video-media">
      <video playsinline="" autoplay="" loop="" muted="" data-autoplay=""
             poster="https://f7ionsy-1251389397.cos.ap-shanghai.myqcloud.com/background/ocean.png" x5-video-player-type="h5">
        <source src="https://f7ionsy-1251389397.cos.ap-shanghai.myqcloud.com/background/ocean.mp4" type="video/mp4">
        <source src="https://f7ionsy-1251389397.cos.ap-shanghai.myqcloud.com/background/ocean.ogv" type="video/ogg">
        <source src="https://f7ionsy-1251389397.cos.ap-shanghai.myqcloud.com/background/ocean.webm" type="video/webm">
        <p>Your user agent does not support the HTML5 Video element.</p>
      </video>
      <div class="video-overlay"></div>
    </div>
    <div class="video-inner text-center text-white">
      
      <!-- motto -->
		<div class="h-body">	
			<% if(theme.motto.enable) { %>
				<p class="motto"></p>
			<% } %>
		</div>
      
    </div>
	
  </div>
</section>
